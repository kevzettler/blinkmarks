<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= f.text_field :email, :class => 'l rm_10 autobox barinput', :value => "Your Email" %></p>

  <%= f.password_field :password, :class => 'l rm_10 autobox barinput', :value=> "Your Password" %></p>

  <%= f.password_field :password_confirmation, :class => 'l rm_10 autobox barinput', :value => 'Password Confirmation' %></p>

  <p><%= f.submit "Sign up", :class => "l button rm_10" %></p>
<% end %>


<%= devise_error_messages! %>
<p class="notice l rm_10"><%= notice %></p>
<p class="alert l rm_10"><%= alert %></p>

