<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <%= f.text_field :email, :value =>"Your Email", :class =>"l rm_10 autobox barinput" %></p>

  <%= f.password_field :password, :value => "Your Password", :class => "l rm_10 autobox barinput" %></p>

  <% if devise_mapping.rememberable? -%>
    <p style="display:none;"><%= f.check_box :remember_me, :checked => "checked" %> <%= f.label :remember_me%></p>
  <% end -%>
	
	<%= f.submit "Sign in", :class => "l rm_10 button" %>
<% end %>

<p class="notice l rm_10"><%= notice %></p>
<p class="alert l rm_10"><%= alert %></p>

<%= link_to "New Account?", new_registration_path(resource_name), :class => "new_account" %>